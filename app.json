{
  "expo": {
    "name": "Fortune",
    "slug": "fortune",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1f1f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "de.datavisions.assetoverview",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "de.datavisions.assetoverview",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f1f1f1"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "7cccbc6d-a1fd-4848-bcf7-542d142ffbb6"
      }
    }
  }
}
